{
  "root": true,
  "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
       "eslint:recommended",
       "prettier",
       "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier", "@typescript-eslint"],
  "env": {
       "es2021": true,
       "node": true
  },
  "rules": {
       "no-console": "error",
       "prettier/prettier": "error"
  }
}